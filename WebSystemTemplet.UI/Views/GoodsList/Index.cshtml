</head>
<body>
    <div style="text-align:center;clear:both;">

    </div>


    <div class="zzsc-content">
        <div class="progress" id="vader1"></div>
        <div class="progress" id="vader2"></div>
        <div class="progress" id="vader3"></div>
        <div class="progress" id="vader4"></div>
        <div class="progress" id="vader5"></div>
    </div>
    @*<article class="zzsc-container"></article>*@
</body>
</html>
<script>
    for (var i = 1; i < 6; i++) {
        $("#vader" + i).ProgressBarWars({ porcentaje: 20 + (i -1)* 20, estilo: "vader" });
    }
</script>