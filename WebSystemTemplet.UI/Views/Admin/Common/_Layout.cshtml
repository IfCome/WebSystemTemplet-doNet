@using WebSystemTemplet.Model
<!doctype html>
<html lang="en">
<head>
    <title>四川张澜职业技术学院-汽车制造与维修技术专业</title>
    <meta name="description" content="四川张澜职业技术学院-汽车制造与维修技术专业">
    <meta name="apple-mobile-web-app-title" content="张澜职业技术学院-汽车制造与维修技术专业" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="keywords" content="index">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="renderer" content="webkit">
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <link rel="icon" type="image/png" href="~/Resources/adminassets/i/favicon.png">
    <link rel="apple-touch-icon-precomposed" href="~/Resources/adminassets/i/app-icon72x72@2x.png">
    @Html.Partial("~/Views/Admin/Common/HeadPart.cshtml")
    @Html.Partial("~/Views/Admin/Common/ScriptPart.cshtml")
</head>

<body>
    <div class="am-g tpl-g">
        <!-- 头部 -->
        <header>
            <!-- logo -->
            <div class="am-fl tpl-header-logo">
                <a href="javascript:;"><img src="~/Resources/adminassets/img/logo.png" alt=""></a>
            </div>
            <!-- 右侧内容 -->
            <div class="tpl-header-fluid">
                <!-- 侧边切换 -->
                <div class="am-fl tpl-header-switch-button am-icon-list">
                    <span>
                    </span>
                </div>
                <!-- 其它功能-->
                <div class="am-fr tpl-header-navbar">
                    <ul>
                        <!-- 欢迎语 -->
                        <li class="am-text-sm tpl-header-navbar-welcome">
                            <marquee direction="left" behavior="scroll" scrollamount="6" scrolldelay="90" onMouseOver="this.scrollamount=2" onMouseOut="this.scrollamount=10">
                                <a>四川张澜职业技术学院 - 汽车制造与维修技术专业 欢迎你</a>
                            </marquee>
                        </li>
                        <!-- 新提示 -->
                        <li class="am-dropdown" data-am-dropdown>
                            <a href="javascript:;" class="am-dropdown-toggle" data-am-dropdown-toggle>
                                <i class="am-icon-bell"></i>
                                <span class="am-badge am-badge-warning am-round item-feed-badge">5</span>
                            </a>
                            <!-- 弹出列表 -->
                            <ul class="am-dropdown-content tpl-dropdown-content">
                                <li class="tpl-dropdown-menu-notifications">
                                    <a href="javascript:;" class="tpl-dropdown-menu-notifications-item am-cf">
                                        <div class="tpl-dropdown-menu-notifications-title">
                                            <i class="am-icon-line-chart"></i>
                                            <span> 有6笔新的销售订单</span>
                                        </div>
                                        <div class="tpl-dropdown-menu-notifications-time">
                                            12分钟前
                                        </div>
                                    </a>
                                </li>
                                <li class="tpl-dropdown-menu-notifications">
                                    <a href="javascript:;" class="tpl-dropdown-menu-notifications-item am-cf">
                                        <div class="tpl-dropdown-menu-notifications-title">
                                            <i class="am-icon-star"></i>
                                            <span> 有3个来自人事部的消息</span>
                                        </div>
                                        <div class="tpl-dropdown-menu-notifications-time">
                                            30分钟前
                                        </div>
                                    </a>
                                </li>
                                <li class="tpl-dropdown-menu-notifications">
                                    <a href="javascript:;" class="tpl-dropdown-menu-notifications-item am-cf">
                                        <div class="tpl-dropdown-menu-notifications-title">
                                            <i class="am-icon-folder-o"></i>
                                            <span> 上午开会记录存档</span>
                                        </div>
                                        <div class="tpl-dropdown-menu-notifications-time">
                                            1天前
                                        </div>
                                    </a>
                                </li>

                                <li class="tpl-dropdown-menu-notifications">
                                    <a href="javascript:;" class="tpl-dropdown-menu-notifications-item am-cf">
                                        <i class="am-icon-bell"></i> 进入列表…
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <!-- 退出 -->
                        <li class="am-text-sm">
                            <a href="javascript:;" onclick="window.location.href = '/Login/Logout.html'">
                                <span class="am-icon-sign-out"></span> 退出
                            </a>
                        </li>
                        <li class="am-text-sm">
                            <a href="javascript:;" id="admin-fullscreen"><span class="am-icon-arrows-alt"></span> <span class="admin-fullText">开启全屏</span></a>
                        </li>
                    </ul>
                </div>
            </div>
        </header>
        <!-- 风格切换 -->
        <div class="tpl-skiner">
            <div class="tpl-skiner-toggle am-icon-cog">
            </div>
            <div class="tpl-skiner-content">
                <div class="tpl-skiner-content-title">
                    选择主题
                </div>
                <div class="tpl-skiner-content-bar">
                    <span class="skiner-color skiner-white" data-color="theme-white"></span>
                    <span class="skiner-color skiner-black" data-color="theme-black"></span>
                </div>
            </div>
        </div>
        <!-- 侧边导航栏 -->
        <div class="left-sidebar">
            <!-- 用户信息 -->
            <div class="tpl-sidebar-user-panel">
                <div class="tpl-user-panel-slide-toggleable">
                    <div class="tpl-user-panel-profile-picture">
                        <img src="@Identity.LoginUserInfo.IconUrl" alt="">
                    </div>
                    <span class="user-panel-logged-in-text">
                        <i class="am-icon-circle-o am-text-success tpl-user-panel-status-icon"></i>
                        @Identity.LoginUserInfo.RealName
                    </span>
                    <a href="javascript:;" class="tpl-user-panel-action-link"> <span class="am-icon-pencil"></span> 账号设置</a>
                </div>
            </div>
            <!-- 菜单 -->
            <ul class="sidebar-nav">
                <li class="sidebar-nav-link">
                    <a id="menu-home" href="/home/indexpage.html">
                        <i class="am-icon-home sidebar-nav-link-logo"></i> 首页
                    </a>
                </li>
                <li class="sidebar-nav-heading">教务<span class="sidebar-nav-heading-info"></span></li>
                <li class="sidebar-nav-link">
                    <a href="javascript:;" class="sidebar-nav-sub-title">
                        <i class="am-icon-users sidebar-nav-link-logo"></i> 人员资料管理
                        <span class="am-icon-chevron-down am-fr am-margin-right-sm sidebar-nav-sub-ico"></span>
                    </a>
                    <ul class="sidebar-nav sidebar-nav-sub">
                        <li class="sidebar-nav-link">
                            <a id="menu-teacherlist" href="/msuserInfo/teacherlist.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 教职工资料
                            </a>
                        </li>
                        <li class="sidebar-nav-link">
                            <a id="menu-studentlist" href="table-list-img.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 学生资料
                            </a>
                        </li>
                        <li class="sidebar-nav-link">
                            <a id="menu-adminlist" href="table-list-img.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 管理员资料
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="sidebar-nav-link">
                    <a href="javascript:;" class="sidebar-nav-sub-title">
                        <i class="am-icon-sitemap sidebar-nav-link-logo"></i> 组织架构
                        <span class="am-icon-chevron-down am-fr am-margin-right-sm sidebar-nav-sub-ico"></span>
                    </a>
                    <ul class="sidebar-nav sidebar-nav-sub">
                        <li class="sidebar-nav-link">
                            <a id="menu-departmentlist" href="/msdepartmentinfo/departmentlist.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 组织架构概览
                            </a>
                        </li>
                        <li class="sidebar-nav-link">
                            <a id="menu-majorlist" href="/msdepartmentinfo/majorlist.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 专业管理
                            </a>
                        </li>
                        <li class="sidebar-nav-link">
                            <a id="menu-classlist" href="/msdepartmentinfo/classlist.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 班级管理
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="sidebar-nav-link">
                    <a href="login.html">
                        <i class="am-icon-table sidebar-nav-link-logo"></i> 课程管理
                    </a>
                </li>

                <li class="sidebar-nav-link">
                    <a href="login.html">
                        <i class="am-icon-graduation-cap sidebar-nav-link-logo"></i> 成绩管理
                    </a>
                </li>
                <li class="sidebar-nav-heading">系统<span class="sidebar-nav-heading-info"></span></li>
                <li class="sidebar-nav-link">
                    <a href="javascript:;" class="sidebar-nav-sub-title">
                        <i class="am-icon-cogs sidebar-nav-link-logo"></i> 系统设置
                        <span class="am-icon-chevron-down am-fr am-margin-right-sm sidebar-nav-sub-ico"></span>
                    </a>
                    <ul class="sidebar-nav sidebar-nav-sub">
                        <li class="sidebar-nav-link">
                            <a href="table-list.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 菜单管理
                            </a>
                        </li>
                        <li class="sidebar-nav-link">
                            <a href="table-list-img.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 角色管理
                            </a>
                        </li>
                        <li class="sidebar-nav-link">
                            <a href="table-list-img.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 权限管理
                            </a>
                        </li>
                        <li class="sidebar-nav-link">
                            <a href="table-list-img.html">
                                <span class="am-icon-angle-right sidebar-nav-link-logo"></span> 岗位权限分配
                            </a>
                        </li>
                    </ul>
                </li>
            </ul>

        </div>

        <!-- 内容区域 -->
        @RenderBody()

        <div class="am-modal am-modal-loading am-modal-no-btn" tabindex="-1" id="my-modal-loading">
            <div class="am-modal-dialog tpl-model-dialog tpl-am-modal-loading">
                <div class="am-modal-hd">正在载入...</div>
                <div class="am-modal-bd">
                    <span class="am-icon-spinner am-icon-spin"></span>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
